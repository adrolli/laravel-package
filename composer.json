{
    "name": "tallui/tallui-package",
    "description": "Test.",
    "keywords": [
        "tallui",
        "tallui-package"
    ],
    "homepage": "https://github.com/tallui/tallui-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Alf Drollinger",
            "email": "alf@drollinger.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "TallUI\\TalluiPackage\\": "src",
            "TallUI\\TalluiPackage\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TallUI\\TalluiPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TallUI\\TalluiPackage\\TalluiPackageServiceProvider"
            ],
            "aliases": {
                "TalluiPackage": "TallUI\\TalluiPackage\\TalluiPackageFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
